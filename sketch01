Table table;
TableRow row;

void setup(){
  size(1920,1080,P3D);
  background(255);
  table = loadTable("bird_sample.csv","header, csv");
  
  String departure;
  String arrival;
  for(int i=0; i < table.getRowCount(); i++){
    row = table.getRow(i);
    departure = row.getString("LATITUDE");
    arrival = row.getString("LONGITUDE");
    print( departure );
    print( arrival );
  }
}

void draw(){
  
  background(frameCount % 255);

}
